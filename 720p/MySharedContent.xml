		<control type="group">
			<!-- background -->
			<control type="group">
				<control type="image">
					<description>Background Overlay</description>
					<include>FullScreenOverlayDims</include>
					<texture>SharedContentBG.png</texture>
					<animation effect="fade" start="0" end="60" time="0" delay="0">Visible</animation>
				</control>
			</control>
			<!-- main list -->
			<control type="group" id="777">
				<control type="fixedlist" id="56">
					<viewtype label="ListSecondary">list</viewtype>
					<posy>235</posy>
					<posx>100</posx>
					<width>1000</width>
					<height>400</height>
					<onright>-</onright>
					<onleft>9100</onleft>
					<onup>40</onup>
					<ondown>40</ondown>
					<focusposition>1</focusposition>
					<wraparound>true</wraparound>
					<scrolltime>200</scrolltime>
					<pagecontrol>61</pagecontrol>
					<itemlayout height="45" width="1000">
						<control type="label">
							<posx>60</posx>
							<width>800</width>
							<height>42</height>
							<align>left</align>
							<aligny>center</aligny>
							<label>$INFO[ListItem.Label]</label>
							<font>Condensed-28</font>
							<textcolor>FF999999</textcolor>
							<shadowcolor>FF222222</shadowcolor>
						</control>
					</itemlayout>
					<focusedlayout height="85" width="1000">
						<control type="label">
							<posx>80</posx>
							<width>800</width>
							<height>80</height>
							<align>left</align>
							<aligny>center</aligny>
							<label>$INFO[ListItem.Label]</label>
							<font>CondensedBold-48</font>
							<scroll>false</scroll>
							<textcolor>orangeLight</textcolor>
							<shadowcolor>FF222222</shadowcolor>
						</control>
					</focusedlayout>
				</control>
			</control>
			<!-- right side thumbs -->
			<control type="group">
				<posx>800</posx>
				<posy>300</posy>
				<visible>!Container.OnNext + !Container.OnPrevious</visible>
				<animation effect="fade" start="0" end="100" time="1000" delay="300" condition="Control.HasFocus(56)" reversible="false">Conditional</animation>
				<animation effect="fade" start="0" end="100" time="2000" delay="0">WindowOpen</animation>
				<animation effect="fade" start="0" end="100" time="400" delay="0">Visible</animation>
				<animation effect="fade" start="0" end="0" time="0">Hidden</animation>
				<!-- selected listitem thumbs -->	
				<control type="grouplist">
					<orientation>horizontal</orientation>
					<itemgap>16</itemgap>					
					<control type="image">
						<width max="200">auto</width>
						<height>350</height>
						<bordertexture border="14">bigPosterFrameShadow.png</bordertexture>
						<bordersize>9</bordersize>
						<texture background="true" diffuse="bigPosterMask.png">$INFO[ListItem(0).Thumb0]</texture>
						<aspectratio scalediffuse="true" align="center" aligny="center">keep</aspectratio>
						<visible>!IsEmpty(ListItem(0).Thumb0)</visible>
					</control>
					<control type="grouplist">
						<orientation>vertical</orientation>
						<itemgap>16</itemgap>
